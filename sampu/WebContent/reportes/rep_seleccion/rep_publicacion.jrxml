	<queryString>
		<![CDATA[select nro_puesto_aprob_spdsp,valor_puesto_spdsp,detalle_gttco,codigo_partida_gepap,cargo_funcional,nom_sucu,
detalle_formacion_academica,detalle_competencia,detalle_experiencia
from (
select ide_spcac,a.ide_spdsp,a.ide_gepap,a.ide_gttco,b.ide_spsop,b.ide_gegca,nro_puesto_aprob_spdsp,valor_puesto_spdsp,
c.detalle_gttco,d.codigo_partida_gepap,cargo_funcional,nom_sucu
from spr_cargo_contratacion a,spr_detalle_solicitud_puesto b,gth_tipo_contrato c,gen_partida_presupuestaria d,
(
SELECT a.ide_gegca,b.detalle_gegro||' '||c.detalle_gecaf as cargo_funcional,d.detalle_gedep,e.detalle_geare FROM gen_grupo_cargo_area a,
gen_grupo_ocupacional b, gen_cargo_funcional c,gen_departamento d,gen_area e
where a.ide_gegro=b.ide_gegro and a.ide_gecaf=c.ide_gecaf and a.ide_gedep=d.ide_gedep and d.ide_geare=e.ide_geare
) e,sis_sucursal f
where a.ide_spdsp = b.ide_spdsp and a.ide_gttco = c.ide_gttco
and a.ide_gepap = d.ide_gepap and b.ide_gegca = e.ide_gegca
and b.ide_sucu = f.ide_sucu
) a
left join (
select ide_spdsp,detalle_cmdec as detalle_experiencia
from spr_perfil_experiencia a, cmp_detalle_competencia b
where a.ide_cmdec = b.ide_cmdec
) b on a.ide_spdsp = b.ide_spdsp
left join (
select  ide_spdsp,detalle_cmdec as detalle_competencia from spr_perfil_competencia  a, cmp_detalle_competencia b
where a.ide_cmdec = b.ide_cmdec
) c on a.ide_spdsp = c.ide_spdsp
left join (
select ide_spdsp,detalle_cmdec as detalle_formacion_academica  from spr_perfil_formacion_academica  a, cmp_detalle_competencia b
where a.ide_cmdec = b.ide_cmdec
) d  on a.ide_spdsp = d.ide_spdsp
WHERE ide_spsop = $P{ide_spsop}]]>
	</queryString>
